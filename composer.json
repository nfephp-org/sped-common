{
    "name": "nfephp-org/sped-common",
    "type": "library",
    "description": "sped-common Ã© parte do NFePHP.",
    "keywords": [
        "nfe",
        "sped",
        "nfephp"
    ],
    "homepage": "https://github.com/nfephp-org/sped-common",
    "license": [
        "GPL-3.0+",
        "LGPL-3.0+"
    ],
    "authors": [
        {
            "name": "Roberto L. Machado",
            "email": "linux.rlm@gmail.com.br",
            "homepage": "http://www.nfephp.org",
            "role": "Developer"
        },
        {
            "name": "Comunidade NFePHP",
            "homepage": "https://github.com/nfephp-org/nfephp/graphs/contributors"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-libxml": "*",
        "ext-xmlreader": "*",
        "ext-curl": "*",
        "ext-soap": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "psr/log": "^1.0",
        "zendframework/zend-servicemanager": "^2.5",
        "zendframework/zend-mail": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "NFePHP\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHP\\Common\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-v5.0": "5.0-dev"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist",
        "cs": "phpcs --standard=PSR2 src"
    }
}
